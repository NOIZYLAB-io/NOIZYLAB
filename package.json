{
  "name": "codemaster",
  "version": "1.0.0",
  "description": "CODEMASTER - The Ultimate AI-Powered Code Command Center",
  "type": "module",
  "bin": {
    "codemaster": "./codemaster.js",
    "noizy": "./noizy.js"
  },
  "scripts": {
    "dev": "wrangler dev workers/email-worker.js",
    "dev:api": "wrangler dev workers/api-dashboard.js --port 8788",
    "dev:codemaster": "wrangler dev workers/codemaster-api.js --port 8789",
    "deploy": "npm run deploy:all",
    "deploy:all": "wrangler deploy workers/email-worker.js && wrangler deploy workers/api-dashboard.js && wrangler deploy workers/codemaster-api.js",
    "deploy:email": "wrangler deploy workers/email-worker.js",
    "deploy:api": "wrangler deploy workers/api-dashboard.js",
    "deploy:codemaster": "wrangler deploy workers/codemaster-api.js",
    "logs": "wrangler tail",
    "test": "node --test tests/",
    "lint": "eslint .",
    "start": "node codemaster.js",
    "shell": "node codemaster.js shell",
    "status": "node codemaster.js status"
  },
  "keywords": [
    "codemaster",
    "ai",
    "agents",
    "cli",
    "cloudflare",
    "workers",
    "code-generation",
    "automation",
    "workflow",
    "email"
  ],
  "author": "NoizyLab",
  "license": "MIT",
  "dependencies": {
    "wrangler": "^3.47.0"
  },
  "devDependencies": {
    "eslint": "^8.57.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Noizyfish/NOIZYLAB.git"
  }
}
