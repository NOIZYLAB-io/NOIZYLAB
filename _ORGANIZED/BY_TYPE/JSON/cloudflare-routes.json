{
  "version": 1,
  "description": "Cloudflare routing rules for NoizyLab OS",
  "routes": [
    {
      "pattern": "ai.noizylab.workers.dev/*",
      "worker": "ai-super-worker",
      "zone": "noizylab.com"
    },
    {
      "pattern": "api.noizylab.workers.dev/*",
      "worker": "super-worker",
      "zone": "noizylab.com"
    },
    {
      "pattern": "intake.noizylab.workers.dev/*",
      "worker": "intake",
      "zone": "noizylab.com"
    },
    {
      "pattern": "mc96.noizylab.workers.dev/*",
      "worker": "mc96",
      "zone": "noizylab.com"
    },
    {
      "pattern": "teamviewer.noizylab.workers.dev/*",
      "worker": "teamviewer",
      "zone": "noizylab.com"
    },
    {
      "pattern": "arbiter.noizylab.workers.dev/*",
      "worker": "agent-arbiter",
      "zone": "noizylab.com"
    },
    {
      "pattern": "dreamchamber.noizylab.workers.dev/*",
      "worker": "dreamchamber",
      "zone": "noizylab.com"
    },
    {
      "pattern": "events.noizylab.workers.dev/*",
      "worker": "events",
      "zone": "noizylab.com"
    }
  ],
  "customDomains": [
    {
      "domain": "ai.noizylab.com",
      "worker": "ai-super-worker"
    },
    {
      "domain": "api.noizylab.com",
      "worker": "super-worker"
    },
    {
      "domain": "intake.noizylab.com",
      "worker": "intake"
    },
    {
      "domain": "mc96.noizylab.com",
      "worker": "mc96"
    }
  ],
  "rateLimiting": {
    "ai-super-worker": {
      "requests_per_minute": 60,
      "burst": 10
    },
    "super-worker": {
      "requests_per_minute": 120,
      "burst": 20
    }
  },
  "caching": {
    "ai-super-worker": {
      "cache_ttl": 300,
      "cache_key": "ai-${request.url}"
    }
  }
}

