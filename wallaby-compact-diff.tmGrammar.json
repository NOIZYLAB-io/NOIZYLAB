
{
  "scopeName": "source.wallaby-compact-diff",
  "patterns": [
    {
      "comment": "insertion",
      "begin": "\u2060",
      "end": "\u2060",
      "name": "markup.inserted markup.underline"
    },
    {
      "comment": "deletion",
      "begin": "\u200d",
      "end": "\u200d",
      "name": "markup.deleted markup.strikethrough"
    },
    {
      "comment": "ellipsis",
      "begin": "\u200b",
      "end": "\u200b",
      "name": "entity.name.function"
    }
  ]
}
