{
  "mcpServers": {
    "fetch": {
      "args": [
        "mcp-server-fetch"
      ],
      "command": "uvx",
      "disabled": false,
      "env": {}
    },
    "git": {
      "args": [
        "-m",
        "mcp_server_git"
      ],
      "command": "python",
      "disabled": false,
      "env": {}
    },
    "memory": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-memory"
      ],
      "command": "npx",
      "env": {
        "MEMORY_FILE_PATH": ""
      }
    },
    "sequential-thinking": {
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ],
      "command": "npx",
      "disabled": false,
      "env": {}
    }
  }
}