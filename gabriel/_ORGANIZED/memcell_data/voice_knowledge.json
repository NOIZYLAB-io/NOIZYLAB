{
  "voice_system": {
    "name": "GABRIEL Voice X2000",
    "version": "2.0",
    "codename": "The Winston Edition",
    "location": "titanhive/voice.py",
    "lines_of_code": 5400,
    "primary_character": "Ian McShane as Winston (John Wick)"
  },
  "engines": {
    "master": {
      "name": "MasterVoiceEngine",
      "description": "Triple-layered premium British voice",
      "layers": [
        {"name": "Jamie Premium", "type": "macOS Siri Neural", "role": "primary"},
        {"name": "OpenAI Onyx", "type": "TTS-1-HD", "role": "depth"},
        {"name": "Edge TTS Thomas", "type": "en-GB-ThomasNeural", "role": "texture"}
      ],
      "processing": {
        "pitch_semitones": -3,
        "pitch_factor": 0.84,
        "tempo_correction": 1.19,
        "eq": {
          "bass_180hz": 4,
          "presence_800hz": 2,
          "treble_4000hz": -4
        },
        "compression": {
          "threshold": "-18dB",
          "ratio": 3,
          "attack": 10,
          "release": 150
        }
      }
    },
    "preset": {
      "name": "VoicePresetEngine",
      "description": "Character voice presets with custom processing",
      "presets": {
        "winston": {
          "description": "Ian McShane Continental Manager",
          "pitch": -3,
          "speed": 0.88,
          "voice": "onyx"
        },
        "commander": {
          "description": "Military commander - authoritative",
          "pitch": -2,
          "speed": 0.95,
          "voice": "onyx"
        },
        "butler": {
          "description": "English butler - refined",
          "pitch": -1,
          "speed": 0.92,
          "voice": "fable"
        },
        "godmode": {
          "description": "Omniscient AI - absolute power",
          "pitch": -4,
          "speed": 0.82,
          "voice": "onyx"
        },
        "narrator": {
          "description": "Documentary narrator",
          "pitch": -1,
          "speed": 0.90,
          "voice": "fable"
        },
        "threat": {
          "description": "Menacing warning",
          "pitch": -4,
          "speed": 0.75,
          "voice": "onyx"
        }
      }
    },
    "smart": {
      "name": "SmartVoice",
      "description": "Intelligent emotion detection",
      "emotions": {
        "urgent": ["emergency", "critical", "immediately", "now"],
        "threatening": ["warning", "danger", "consequence", "regret"],
        "authoritative": ["command", "execute", "deploy", "initiate"],
        "calm": ["welcome", "thank", "please", "appreciate"],
        "powerful": ["unlimited", "absolute", "infinite", "god"],
        "storytelling": ["once", "story", "journey", "adventure"]
      }
    },
    "mixer": {
      "name": "VoiceMixer",
      "description": "Layer multiple voices for rich sound",
      "features": ["multi-layer", "stereo-pan", "volume-control"]
    },
    "dynamic": {
      "name": "DynamicVoice",
      "description": "Real-time content adaptation",
      "features": ["emphasis-detection", "pacing", "context-awareness"]
    }
  },
  "backends": {
    "priority": [
      {"name": "Jamie Premium", "type": "macOS", "free": true},
      {"name": "OpenAI TTS-1-HD", "type": "API", "free": false},
      {"name": "Edge TTS", "type": "API", "free": true},
      {"name": "macOS say", "type": "local", "free": true}
    ]
  },
  "quick_functions": {
    "say": "Quick speak with preset",
    "smart": "Auto-detect emotion and speak",
    "mix": "Speak with mixed voices",
    "deep": "Extra deep layered voice",
    "narrate": "Paced narration",
    "dynamic": "Dynamic adaptation"
  },
  "winston_phrases": {
    "categories": [
      "greet", "acknowledge", "thinking", "success",
      "warning", "threat", "farewell", "wisdom",
      "godmode", "casual", "impressed", "disappointed",
      "humor", "action", "continental"
    ],
    "examples": {
      "greet": "Ah. There you are.",
      "threat": "I'll say this only once.",
      "farewell": "Until next time.",
      "godmode": "I am the system."
    }
  },
  "cli": {
    "launcher": "gab",
    "commands": {
      "say": "Master triple-voice",
      "winston": "Winston preset",
      "commander": "Commander preset",
      "butler": "Butler preset",
      "god": "God mode preset",
      "threat": "Threat preset",
      "smart": "Auto-detect emotion",
      "mix": "Mixed layered voice",
      "deep": "Extra deep voice",
      "narrate": "Paced narration",
      "presets": "Demo all presets",
      "realtime": "Low latency mode",
      "demo": "Full demo"
    },
    "shortcuts": {
      "s": "say",
      "w": "winston",
      "g": "god",
      "t": "threat",
      "a": "smart",
      "m": "mix",
      "p": "presets",
      "rt": "realtime"
    }
  },
  "ffmpeg_filter": "asetrate=44100*{pitch},aresample=44100,atempo={tempo},equalizer=f=180:t=q:w=1:g={bass},equalizer=f=800:t=q:w=1.5:g={presence},equalizer=f=4000:t=q:w=2:g={treble},acompressor=threshold=-18dB:ratio=3:attack=10:release=150",
  "cache_locations": {
    "master": "/tmp/gabriel_master/",
    "presets": "/tmp/gabriel_presets/",
    "mixer": "/tmp/gabriel_mixer/"
  }
}
