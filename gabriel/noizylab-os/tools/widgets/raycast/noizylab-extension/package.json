{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "noizylab-os",
  "title": "NoizyLab OS",
  "description": "Control NoizyLab OS - 57 AI Workers for hardware restoration",
  "icon": "extension-icon.png",
  "author": "noizylab",
  "categories": ["Developer Tools", "Productivity"],
  "license": "MIT",
  "commands": [
    {
      "name": "status",
      "title": "Worker Status",
      "subtitle": "NoizyLab OS",
      "description": "View status of all 57 AI workers",
      "mode": "view"
    },
    {
      "name": "ask",
      "title": "Ask AI",
      "subtitle": "NoizyLab OS",
      "description": "Ask any computing question",
      "mode": "view",
      "arguments": [
        {
          "name": "question",
          "placeholder": "What is branch prediction?",
          "type": "text",
          "required": true
        }
      ]
    },
    {
      "name": "cpu",
      "title": "CPU Architecture",
      "subtitle": "NoizyLab OS",
      "description": "Query CPU architecture knowledge",
      "mode": "view"
    },
    {
      "name": "gpu",
      "title": "GPU Computing",
      "subtitle": "NoizyLab OS",
      "description": "Query GPU computing knowledge",
      "mode": "view"
    },
    {
      "name": "os",
      "title": "Operating Systems",
      "subtitle": "NoizyLab OS",
      "description": "Query OS history and knowledge",
      "mode": "view"
    },
    {
      "name": "deploy",
      "title": "Deploy Workers",
      "subtitle": "NoizyLab OS",
      "description": "Deploy all 57 workers to Cloudflare",
      "mode": "no-view"
    },
    {
      "name": "workers",
      "title": "Browse Workers",
      "subtitle": "NoizyLab OS",
      "description": "Browse all 57 AI workers",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.64.0",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@raycast/eslint-config": "^1.0.8",
    "@types/node": "20.10.0",
    "@types/react": "18.2.45",
    "eslint": "^8.55.0",
    "typescript": "^5.3.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "lint": "ray lint"
  }
}
