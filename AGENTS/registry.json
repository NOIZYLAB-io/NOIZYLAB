{
  "version": "1.1.0",
  "updated": "2025-12-24",
  "note": "Paths use $NOIZYLAB_HOME env var. Set this to your NOIZYLAB root directory.",
  "agents": [
    {
      "id": "gabriel",
      "name": "GABRIEL",
      "description": "Zero Latency Voice + Control Agent",
      "entrypoint": "tools/gabriel_control.py",
      "path": "$NOIZYLAB_HOME/GABRIEL",
      "status": "active",
      "capabilities": ["voice", "ui", "automation"]
    },
    {
      "id": "mc96",
      "name": "MC96",
      "description": "Core Universe Engine",
      "entrypoint": "vault/organize_code.py",
      "path": "$NOIZYLAB_HOME/MC96",
      "status": "active",
      "capabilities": ["code-organization", "vault"]
    },
    {
      "id": "system-guardian",
      "name": "SystemGuardian",
      "description": "System Monitoring Agent",
      "entrypoint": null,
      "path": "$NOIZYLAB_HOME/SystemGuardian",
      "status": "stub",
      "capabilities": ["monitoring", "alerts"]
    }
  ],
  "launcher": {
    "gabriel": "python3 $NOIZYLAB_HOME/GABRIEL/tools/gabriel_control.py",
    "mc96": "python3 $NOIZYLAB_HOME/MC96/vault/organize_code.py",
    "media-push": "$NOIZYLAB_HOME/AGENTS/sync-media.sh push",
    "media-pull": "$NOIZYLAB_HOME/AGENTS/sync-media.sh pull",
    "media-status": "$NOIZYLAB_HOME/AGENTS/sync-media.sh status"
  },
  "storage": {
    "code": "https://github.com/NOIZYLAB-io/NOIZYLAB",
    "media": "$NOIZYLAB_GDRIVE"
  }
}
