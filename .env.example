# GoDaddy API Credentials
# Get these from: https://developer.godaddy.com/keys
GODADDY_API_KEY=your_api_key_here
GODADDY_API_SECRET=your_api_secret_here

# Domain Configuration
DOMAIN=noizylab.ca

# Mail Server Configuration
MAIL_SERVER_IP=YOUR_MAIL_SERVER_IP

# DKIM Configuration
# Generate DKIM keys with: npm run generate:dkim
DKIM_SELECTOR=selector1
DKIM_PUBLIC_KEY=your_dkim_public_key_here
DKIM_PRIVATE_KEY=your_dkim_private_key_here

# Email Test Configuration (optional)
# Configure these to test email sending functionality
SMTP_HOST=smtp.example.com
SMTP_PORT=587
SMTP_USER=your_email@noizylab.ca
SMTP_PASS=your_smtp_password
TEST_EMAIL_TO=test@example.com
